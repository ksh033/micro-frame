{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap.js",
    "build": "pnpm --filter=!@micro-frame/sc-web build",
  
    "build2": "pnpm --filter  @micro-frame/sc-slave build:dev",
    "build3": "pnpm --filter  @micro-frame/sc-runtime build:dev",
    "build4": "pnpm --filter  @micro-frame/plugin-microlayout build:dev",
    "release": "node ./scripts/release.js --conventionalPrerelease=true",
    "build:dev": "pnpm --filter  @micro-frame/sc-slave  --filter  @micro-frame/sc-runtime   --filter  @micro-frame/plugin-microlayout build:dev",
    "check-deps": "node ./scripts/checkDeps.js",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx  --fix --format=pretty ./ && npm run lint:prettier",
    "lint:prettier": "npm run prettier &&  git diff && prettier --version && prettier --check \"packages/**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,yaml,json}\" --loglevel warn",
    "build:web-dev": "npm run build && cross-env  REACT_APP_ENV=dev APP_ROOT=packages/web umi build",
    "build:web-pro": "npm run build && cross-env REACT_APP_ENV=pro APP_ROOT=packages/web umi build",
    "build:web-test": "npm run build && cross-env REACT_APP_ENV=test APP_ROOT=packages/web umi build",
    "build:web-pre": "npm run build && cross-env REACT_APP_ENV=pre APP_ROOT=packages/web umi build",
    "start": "pnpm  cross-env REACT_APP_ENV=dev APP_ROOT=packages/web max dev",
    "update:deps": "yarn upgrade"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "resolutions": {},
  "devDependencies": {
    "@ant-design/compatible": "^5.1.1",
    "@babel/core": "^7.22.5",
    "@babel/parser": "^7.22.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "@types/classnames": "^2.3.1",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/enzyme": "^3.10.13",
    "@types/history": "^4.7.11",
    "@types/jest": "^26.0.24",
    "@types/memoize-one": "^5.1.2",
    "@types/node": "^14.18.51",
    "@types/react": "18.0.33",
    "@types/react-dom": "^18.2.5",
    "@types/react-helmet": "^6.1.6",
    "@types/react-intl": "^3.0.0",
    "@types/react-resizable": "^1.7.4",
    "@types/react-responsive": "^8.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/warning": "^3.0.0",
    "@types/xhr-mock": "^2.0.0",
    "@umijs/fabric": "^4.0.1",
    "@umijs/lint": "^4.0.70",
    "@umijs/max": "^4.0.70",
    "@umijs/preset-umi": "^4.0.70",
    "@umijs/utils": "^4.0.70",
    "antd": "^5.6.1",
    "array-move": "^4.0.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-lodash": "3.3.4",
    "babel-types": "^6.26.0",
    "chalk": "^5.2.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "execa": "^5.1.1",
    "father": "^4.2.3",
    "glob": "^10.2.7",
    "inquirer": "^8.2.5",
    "jsdom": "^16.7.0",
    "jsdom-global": "^3.0.2",
    "jsencrypt": "^3.3.2",
    "lerna": "^6.5.1",
    "lerna-changelog": "^2.2.0",
    "less-loader": "^8.1.1",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "np": "^6.5.0",
    "pify": "^5.0.0",
    "prettier": "~2.8.8",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-packagejson": "^2.4.3",
    "pretty-quick": "^3.1.3",
    "progress-bar-webpack-plugin": "^2.1.0",
    "query-string": "^6.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "slash2": "^2.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4",
    "umi": "^4.0.70"
  },
  "packageManager": "pnpm@8.2.0",
  "name": "micro-frame"
}
