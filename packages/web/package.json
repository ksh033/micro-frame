{
  "name": "@micro-frame/sc-web",
  "version": "1.0.0-beta.27",
  "description": "@micro-frame/sc-web",
  "module": "es/index.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "dist",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design/pro-components"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "scripts": {
    "build-runtime": "father-build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env REACT_APP_ENV=pro umi build",
    "build:test": "cross-env  REACT_APP_ENV=test umi build",
    "build:pre": "cross-env  REACT_APP_ENV=pre umi build",
    "build:dev": "cross-env   REACT_APP_ENV=dev umi build",
    "link": "yarn link sc-element",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev MOCK=none FRIENDLY_ERROR=none PORT=9999",
    "start:mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "ui": "umi ui"
  },
  "keywords": [],
  "authors": [],
  "dependencies": {
    "@ant-design/colors": "^3.1.0",
    "@ant-design/icons": "^4.0.0",
    "@scboson/sc-schema": "beta",
    "ahooks": "^2.2.0",
    "classnames": "^2.2.6",
    "es6-promise": "^4.2.8",
    "global": "^4.4.0",
    "jsencrypt": "2.3.1",
    "lodash": "^4.17.11",
    "path-to-regexp": "2.4.0",
    "qs": "^6.9.0",
    "react-helmet": "^5.2.1",
    "umi": "^3.2.10",
    "umi-request": "^1.2.7",
    "use-media-antd-query": "^1.0.1"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^8.0.0",
    "@umijs/plugin-antd": "^0.9.0",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/plugin-locale": "^0.14.2",
    "@umijs/plugin-model": "^2.5.7",
    "@umijs/plugin-qiankun": "^2.33.1"
  },
  "license": "MIT",
  "peerDependencies": {
    "umi": "3.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
