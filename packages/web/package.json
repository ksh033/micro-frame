{
  "name": "@micro-frame/sc-web",
  "version": "1.0.1-beta.69",
  "description": "@micro-frame/sc-web",
  "module": "es/index.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "dist",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design/pro-components"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "scripts": {
    "build-runtime": "father-build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env REACT_APP_ENV=pro umi build",
    "build:test": "cross-env  REACT_APP_ENV=test umi build",
    "build:pre": "cross-env  REACT_APP_ENV=pre umi build",
    "build:dev": "cross-env   REACT_APP_ENV=dev umi build",
    "link": "yarn link sc-element",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "max dev",
    "start:mock": "cross-env MOCK=none max dev",
    "start:no-ui": "cross-env UMI_UI=none max dev",
    "ui": "umi ui"
  },
  "keywords": [],
  "authors": [],
  "dependencies": {
    "@ant-design/compatible": "^5.1.1",
    "@ant-design/icons": "^5.0.1",
    "@scboson/sc-element": "link:../../sc-boson/packages/element",
    "@scboson/sc-layout": "link:../../sc-boson/packages/layout",
    "@scboson/sc-schema": "link:../../sc-boson/packages/schema",

    "@micro-frame/sc-runtime": "workspace:*",
    "@umijs/max": "^4.0.64",
    "ahooks": "^3.7.6",
    "antd": "^5.4.2",
    "classnames": "^2.3.2",
    "jsencrypt": "^3.3.2",
    "react": "18.x",
    "react-dnd": "^11.1.3"
  },
  "license": "MIT",
  "peerDependencies": {
    "umi": "4.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
